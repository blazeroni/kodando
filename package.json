    "publish:kodando-react-dom": "npm publish ./kodando-modules/react-dom",
    "publish:kodando-crossroads": "npm publish ./modules/kodando-crossroads",
    "publish:kodando-hasher": "npm publish ./modules/kodando-hasher",
